							<li><%= link_to "Service", static_service_path %></li>
							<li><a href="#">Graphic Design</a></li>
							<li><a href="#">Icon Design</a></li>
							<li><a href="#">WordPress Theme</a></li>
							
						</ul>
					</li>
						<li><a href="pricing.html">Pricing</a></li>
